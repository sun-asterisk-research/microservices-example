{
  "name": "users",
  "packageManager": "yarn@3.3.1",
  "devDependencies": {
    "@types/node": "^18.11.18",
    "tsc-watch": "^6.0.0",
    "typescript": "^4.9.4"
  },
  "dependencies": {
    "fastify": "^4.11.0",
    "fluent-json-schema": "^4.0.0",
    "hasura-node-types": "^0.2.1",
    "module-alias": "^2.2.2"
  },
  "_moduleAliases": {
    "#routes": "dist/routes",
    "#controllers": "dist/controllers",
    "#schema": "dist/schema"
  },
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "dev": "tsc-watch -p tsconfig.json --onSuccess \"node ./dist/index.js\"",
    "start": "node dist/index.js"
  }
}
