{
  "name": "web",
  "private": true,
  "sideEffects": false,
  "scripts": {
    "build": "remix build",
    "deploy": "wrangler publish",
    "codegen": "graphql-codegen --require dotenv/config",
    "dev": "npm run codegen && run-p \"dev:*\"",
    "start": "cross-env NODE_ENV=production miniflare ./build/index.js",
    "dev:watch": "remix watch",
    "dev:miniflare": "cross-env NODE_ENV=development miniflare ./build/index.js --watch",
    "dev:codegen": "npm run codegen -- -w",
    "postinstall": "remix-esbuild-override"
  },
  "dependencies": {
    "@apollo/client": "^3.7.0",
    "@emotion/react": "^11.10.4",
    "@emotion/server": "^11.10.0",
    "@faker-js/faker": "^7.6.0",
    "@mantine/core": "^5.4.1",
    "@mantine/form": "^6.0.0",
    "@mantine/hooks": "^5.4.1",
    "@mantine/remix": "^5.4.1",
    "@remix-run/cloudflare": "^1.13.0",
    "@remix-run/cloudflare-workers": "^1.13.0",
    "@remix-run/react": "^1.7.0",
    "@tabler/icons": "^1.97.0",
    "@tabler/icons-react": "^2.8.0",
    "@types/lodash": "^4.14.186",
    "cross-env": "^7.0.3",
    "graphql": "^16.6.0",
    "lodash": "^4.17.21",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-infinite-scroll-component": "^6.1.0"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^3.14.1",
    "@esbuild-plugins/node-globals-polyfill": "^0.1.1",
    "@graphql-codegen/cli": "^2.16.3",
    "@graphql-codegen/client-preset": "^1.2.5",
    "@graphql-codegen/introspection": "^2.2.3",
    "@graphql-codegen/typescript": "^2.8.7",
    "@graphql-codegen/typescript-operations": "^2.5.12",
    "@graphql-codegen/typescript-react-apollo": "^3.3.7",
    "@remix-run/dev": "^1.13.0",
    "@remix-run/eslint-config": "^1.13.0",
    "@types/react": "^17.0.47",
    "@types/react-dom": "^17.0.17",
    "eslint": "^8.20.0",
    "graphqurl": "^1.0.1",
    "miniflare": "^2.6.0",
    "npm-run-all": "^4.1.5",
    "remix-esbuild-override": "^3.0.4",
    "typescript": "^4.7.4",
    "wrangler": "^2.0.22"
  },
  "engines": {
    "node": ">=16.13"
  }
}
